<mujoco model="planar_2dof">
    <compiler angle="radian"/>

    <option gravity="0 0 0" integrator="implicitfast" timestep="0.01">
        <flag contact="enable"/>
    </option>

    <worldbody>
        <geom size="0.1" pos="0 0 0.1"/>
        <body name="link0" pos="0 0 0.1">
            <inertial pos="0 0 0.5" mass="1" diaginertia="0.0858 0.0858 0.005"/>
            <joint name="joint1" pos="0 0 0" axis="1 0 0" range="-2.8973 2.8973" actuatorfrcrange="-87 87"/>
            <geom size="0.1 0.5" pos="0 0 0.6" type="cylinder"/>
            <body name="link1" pos="0 0 1.1">
                <inertial pos="0 0 0.50007" mass="1.0001" diaginertia="0.0858997 0.0858997 0.00505"/>
                <joint name="joint2" pos="0 0 0" axis="1 0 0" range="-1.7628 1.7628" actuatorfrcrange="-87 87"/>
                <geom size="0.1 0.5" pos="0 0 0.6" type="cylinder"/>
                <geom size="0.1" pos="0 0 1.2"/>
                <body name="link2" pos="0 0 1.1">
                    <inertial pos="0 0 0.005" mass="0.0001" diaginertia="0.00005 0.00005 0.00005"/>
                    <joint name="joint3" pos="0 0 0" axis="1 0 0" range="-1.7628 1.7628" actuatorfrcrange="-87 87"/>
                    <geom name="tip" size="0.1" pos="0 0 0.005" type="sphere"/>
                    <site name="tip" pos="0 0 0.005" size="0.01"/>
                </body>
            </body>
        </body>

        <body name="obstacle_0" mocap="true" pos="0.0 -0.9 0.3" quat="1 0 0 0">
            <geom type="sphere" name="obstacle_0" size=".1" rgba="1 0 0 .5"/>
        </body>
        <body name="obstacle_1" mocap="true" pos="0.0 1.2 1.4" quat="1 0 0 0">
            <geom type="sphere" name="obstacle_1" size=".3" rgba="1 0 0 .5"/>
        </body>

        <body name="target" mocap="true" pos="0.0 0. 0.3" quat="1 0 0 0">
            <geom type="sphere" name="target" size=".01" rgba="1 0 0 .5"/>
        </body>
    </worldbody>

    <visual>
        <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0"/>
        <rgba haze="0.15 0.25 0.35 1"/>
        <global azimuth="120" elevation="-20"/>
    </visual>

    <asset>
        <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
        <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
                 markrgb="0.8 0.8 0.8" width="300" height="300"/>
        <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.2"/>
    </asset>

    <worldbody>
        <light pos="0 0 1.5" dir="0 0 -1" directional="true"/>
        <geom name="floor" size="0 0 0.05" type="plane" material="groundplane"/>
    </worldbody>

    <actuator>
        <velocity joint="joint1" ctrlrange="-2.0 2.0" ctrllimited="true" kv="500"/>
        <velocity joint="joint2" ctrlrange="-2.0 2.0" ctrllimited="true" kv="500"/>
    </actuator>
</mujoco>
