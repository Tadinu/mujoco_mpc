<mujoco model="Particle">
  <option timestep="0.01">
    <flag contact="enable"/>
  </option>

  <default>
    <joint type="hinge" axis="0 0 1" limited="true" range="-.29 .29" damping="1"/>
    <motor gear=".1" ctrlrange="-1 1" ctrllimited="true"/>
  </default>

  <worldbody>
    <body name="goal" mocap="true" pos="0.25 0 0.01" quat="1 0 0 0">
        <geom type="sphere" name="goal" size=".01" contype="0" conaffinity="0" rgba="0 1 0 .5"/>
    </body>
    <body name="obstacle_0" mocap="true" pos="-0.25 0 0.01" quat="1 0 0 0">
      <geom type="sphere" name="obstacle_0" size=".01" rgba="1 0 0 .5"/>
    </body>
    <body name="obstacle_1" mocap="true" pos="0.17 0.05 0.01" quat="1 0 0 0">
        <geom type="sphere" name="obstacle_1" size=".01" rgba="1 0 0 .5"/>
    </body>
    <body name="obstacle_2" mocap="true" pos="0.1 -0.05 0.01" quat="1 0 0 0">
        <geom type="sphere" name="obstacle_2" size=".01" rgba="1 0 0 .5"/>
    </body>
    <body name="obstacle_3" mocap="true" pos="0.05 0.07 0.01" quat="1 0 0 0">
        <geom type="sphere" name="obstacle_3" size=".01" rgba="1 0 0 .5"/>
    </body>
    <body name="obstacle_4" mocap="true" pos="-0.07 -0.07 0.01" quat="1 0 0 0">
        <geom type="sphere" name="obstacle_4" size=".01" rgba="1 0 0 .5"/>
    </body>
    <body name="obstacle_5" mocap="true" pos="-0.14 0.09 0.01" quat="1 0 0 0">
      <geom type="sphere" name="obstacle_5" size=".01" rgba="1 0 0 .5"/>
    </body>
    <body name="obstacle_6" mocap="true" pos="-0.21 -0.09 0.01" quat="1 0 0 0">
      <geom type="sphere" name="obstacle_6" size=".01" rgba="1 0 0 .5"/>
    </body>
    <body name="obstacle_7" mocap="true" pos="-0.25 0.03 0.01" quat="1 0 0 0">
      <geom type="sphere" name="obstacle_7" size=".01" rgba="1 0 0 .5"/>
    </body>
    <body name="obstacle_8" mocap="true" pos="-0.2 -0.03 0.01" quat="1 0 0 0">
      <geom type="sphere" name="obstacle_8" size=".01" rgba="1 0 0 .5"/>
    </body>
    <body name="obstacle_9" mocap="true" pos="0.25 0.1 0.01" quat="1 0 0 0">
      <geom type="sphere" name="obstacle_9" size=".01" rgba="1 0 0 .5"/>
    </body>
    <light name="light" pos="0 0 1"/>
    <camera name="fixed" pos="0 0 .75" quat="1 0 0 0"/>
    <geom name="ground" type="plane" contype="0" conaffinity="0" pos="0 0 0" size=".3 .3 .1" material="blue_grid"/>
    <geom name="wall_x" type="plane" contype="0" conaffinity="0" pos="-.3 0 .02" zaxis="1 0 0"  size=".02 .3 .02" material="decoration"/>
    <geom name="wall_y" type="plane" contype="0" conaffinity="0" pos="0 -.3 .02" zaxis="0 1 0"  size=".3 .02 .02" material="decoration"/>
    <geom name="wall_neg_x" type="plane" contype="0" conaffinity="0" pos=".3 0 .02" zaxis="-1 0 0"  size=".02 .3 .02" material="decoration"/>
    <geom name="wall_neg_y" type="plane" contype="0" conaffinity="0" pos="0 .3 .02" zaxis="0 -1 0"  size=".3 .02 .02" material="decoration"/>

    <body name="rigidmass" pos="0 0 0.006">
      <camera name="cam0" pos="0 -0.3 0.3" xyaxes="1 0 0 0 0.7 0.7"/>
      <inertial pos="0 0 0" mass="0.1" diaginertia="1 1 1"/>
      <joint name="root_x" type="slide" pos="0 0 0" axis="1 0 0" range="-5 5"/>
      <body name="base_link_y">
        <inertial pos="0 0 0" mass="0.1" diaginertia="1 1 1"/>
        <joint name="root_y" type="slide" pos="0 0 0" axis="0 1 0" range="-5 5"/>
        <body name="base_link">
          <inertial pos="0 0.000497512 0.0500025" quat="1 -5.07614e-05 0 0" mass="0.1" diaginertia="3.50995 3.5 3.00995"/>
          <joint name="root_theta" pos="0 0 0" axis="0 0 1" range="-5 5"/>
          <geom name="rigidmass" type="cylinder" size="0.02 0.005" rgba="1 0 0 1"/>
          <geom type="cylinder" size="0.002 0.005" pos="0 0 0.001"/>
          <geom type="cylinder" size="0.002 0.0005" rgba="0 0 1 1" pos="0 0.01 0.00505"/>
          <site name="tip" pos="0 0 0" size="0.01"/> <!-- For accelerometer defined in task.xml -->
          <body name="lidar_link">
            <inertial pos="0 0 0" mass="100" diaginertia="1 1 1"/>
            <geom name="lidar_geom" type="cylinder" size="0.01 0.002" rgba="0 0 1 1"/>
            <body name="lidar_sensor_link"/>
          </body>
          <body name="feature_link">
            <inertial pos="0 0 0" mass="0.1" diaginertia="1 1 1"/>
            <geom type="cylinder" size="0.0001 0.002" rgba="0 1 0 1"/>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <velocity name="x_motor" joint="root_x" ctrllimited="true" ctrlrange="-2 2" kv="50"/>
    <velocity name="y_motor" joint="root_y" ctrllimited="true" ctrlrange="-2 2" kv="50"/>
    <velocity name="z_motor" joint="root_theta" ctrllimited="true" ctrlrange="-2 2" kv="50"/>
  </actuator>
</mujoco>
