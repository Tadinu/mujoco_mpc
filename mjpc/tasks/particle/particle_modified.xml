<mujoco model="Particle">
  <option timestep="0.01">
    <flag contact="enable" multiccd="enable"/>
  </option>

  <default>
    <joint type="hinge" axis="0 0 1" limited="true" range="-.29 .29" damping="1"/>
    <motor gear=".1" ctrlrange="-1 1" ctrllimited="true"/>
  </default>

  <worldbody>
    <body name="goal_mocap" mocap="true" pos="0.25 0 0.01">
        <geom type="sphere" name="goal_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="goal" pos="0.25 0 0.01">
        <joint type="slide"  pos="0 0 0" axis="1 0 0" />
        <joint type="slide"  pos="0 0 0" axis="0 1 0" />
        <geom type="sphere" name="goal" size=".01" rgba="0 1 0 .5"/>
    </body>

    <body name="obstacle_0_mocap" mocap="true" pos="-0.25 0 0.01">
        <geom type="sphere" name="obstacle_0_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_0" pos="-0.25 0 0.01" quat="1 0 0 0">
        <joint type="slide"  pos="0 0 0" axis="1 0 0" />
        <joint type="slide"  pos="0 0 0" axis="0 1 0" />
        <geom type="sphere" name="obstacle_0" size=".01" rgba="1 0 0 .5"/>
    </body>

    <body name="obstacle_1_mocap" mocap="true" pos="0.17 0.05 0.01">
        <geom type="sphere" name="obstacle_1_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_1" pos="0.17 0.05 0.01" quat="1 0 0 0">
        <joint type="slide"  pos="0 0 0" axis="1 0 0" />
        <joint type="slide"  pos="0 0 0" axis="0 1 0" />
        <geom type="sphere" name="obstacle_1" size=".01" rgba="1 0 0 .5"/>
    </body>

    <body name="obstacle_2_mocap" mocap="true" pos="0.1 -0.05 0.01">
        <geom type="sphere" name="obstacle_2_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_2" pos="0.1 -0.05 0.01" quat="1 0 0 0">
       <joint type="slide"  pos="0 0 0" axis="1 0 0" />
       <joint type="slide"  pos="0 0 0" axis="0 1 0" />
       <geom type="sphere" name="obstacle_2" size=".01" rgba="1 0 0 .5"/>
    </body>

    <body name="obstacle_3_mocap" mocap="true" pos="0.05 0.07 0.01">
        <geom type="sphere" name="obstacle_3_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_3" pos="0.05 0.07 0.01" quat="1 0 0 0">
        <joint type="slide"  pos="0 0 0" axis="1 0 0" />
        <joint type="slide"  pos="0 0 0" axis="0 1 0" />
        <geom type="sphere" name="obstacle_3" size=".01" rgba="1 0 0 .5"/>
    </body>

    <body name="obstacle_4_mocap" mocap="true" pos="-0.07 -0.07 0.01">
        <geom type="sphere" name="obstacle_4_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_4" pos="-0.07 -0.07 0.01" quat="1 0 0 0">
        <joint type="slide"  pos="0 0 0" axis="1 0 0" />
        <joint type="slide"  pos="0 0 0" axis="0 1 0" />
        <geom type="sphere" name="obstacle_4" size=".01" rgba="1 0 0 .5"/>
    </body>

    <body name="obstacle_5_mocap" mocap="true" pos="-0.14 0.09 0.01">
        <geom type="sphere" name="obstacle_5_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_5" pos="-0.14 0.09 0.01" quat="1 0 0 0">
        <joint type="slide"  pos="0 0 0" axis="1 0 0" />
        <joint type="slide"  pos="0 0 0" axis="0 1 0" />
        <geom type="sphere" name="obstacle_5" size=".01" rgba="1 0 0 .5"/>
    </body>

    <body name="obstacle_6_mocap" mocap="true" pos="-0.21 -0.09 0.01">
      <geom type="sphere" name="obstacle_6_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_6" pos="-0.21 -0.09 0.01">
      <joint type="slide"  pos="0 0 0" axis="1 0 0" />
      <joint type="slide"  pos="0 0 0" axis="0 1 0" />
      <geom type="sphere" name="obstacle_6" size=".01" rgba="1 0 0 .5"/>
    </body>

    <body name="obstacle_7_mocap" mocap="true" pos="-0.25 0.03 0.01">
      <geom type="sphere" name="obstacle_7_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_7" pos="-0.25 0.03 0.01">
      <joint type="slide"  pos="0 0 0" axis="1 0 0" />
      <joint type="slide"  pos="0 0 0" axis="0 1 0" />
      <geom type="sphere" name="obstacle_7" size=".01" rgba="1 0 0 .5"/>
    </body>

    <body name="obstacle_8_mocap" mocap="true" pos="-0.2 -0.03 0.01">
      <geom type="sphere" name="obstacle_8_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_8" pos="-0.2 -0.03 0.01">
      <joint type="slide"  pos="0 0 0" axis="1 0 0" />
      <joint type="slide"  pos="0 0 0" axis="0 1 0" />
      <geom type="sphere" name="obstacle_8" size=".01" rgba="1 0 0 .5"/>
    </body>

    <body name="obstacle_9_mocap" mocap="true" pos="0.25 0.1 0.01">
      <geom type="sphere" name="obstacle_9_mocap" size=".01" rgba="0 0 0 0"/>
    </body>
    <body name="obstacle_9" pos="0.25 0.1 0.01">
      <joint type="slide"  pos="0 0 0" axis="1 0 0" />
      <joint type="slide"  pos="0 0 0" axis="0 1 0" />
      <geom type="sphere" name="obstacle_9" size=".01" rgba="1 0 0 .5"/>
    </body>

    <light name="light" pos="0 0 1"/>
    <camera name="fixed" pos="0 0 .75" quat="1 0 0 0"/>
    <geom name="ground" type="plane" contype="0" conaffinity="0" pos="0 0 0" size=".3 .3 .1" material="blue_grid"/>
    <geom name="wall_x" type="plane" contype="0" conaffinity="0" pos="-.3 0 .02" zaxis="1 0 0"  size=".02 .3 .02" material="decoration"/>
    <geom name="wall_y" type="plane" contype="0" conaffinity="0" pos="0 -.3 .02" zaxis="0 1 0"  size=".3 .02 .02" material="decoration"/>
    <geom name="wall_neg_x" type="plane" contype="0" conaffinity="0" pos=".3 0 .02" zaxis="-1 0 0"  size=".02 .3 .02" material="decoration"/>
    <geom name="wall_neg_y" type="plane" contype="0" conaffinity="0" pos="0 .3 .02" zaxis="0 -1 0"  size=".3 .02 .02" material="decoration"/>

    <body name="rigidmass" pos="0 0 .01">
      <camera name="cam0" pos="0 -0.3 0.3" xyaxes="1 0 0 0 0.7 0.7"/>
      <joint name="root_x" type="slide"  pos="0 0 0" axis="1 0 0" />
      <joint name="root_y" type="slide"  pos="0 0 0" axis="0 1 0" />
      <geom name="rigidmass" type="sphere" size=".01" material="self" mass="0.03"/>
      <site name="tip" pos="0 0 0" size="0.01"/>
    </body>
  </worldbody>

  <contact>
    <exclude body1="goal_mocap" body2="goal"/>
    <exclude body1="goal" body2="rigidmass"/>
    <exclude body1="goal_mocap" body2="rigidmass"/>

    <exclude body1="goal" body2="obstacle_0"/>
    <exclude body1="goal" body2="obstacle_1"/>
    <exclude body1="goal" body2="obstacle_2"/>
    <exclude body1="goal" body2="obstacle_3"/>
    <exclude body1="goal" body2="obstacle_4"/>
    <exclude body1="goal" body2="obstacle_5"/>
    <exclude body1="goal" body2="obstacle_6"/>
    <exclude body1="goal" body2="obstacle_7"/>
    <exclude body1="goal" body2="obstacle_8"/>
    <exclude body1="goal" body2="obstacle_9"/>

    <exclude body1="goal_mocap" body2="obstacle_0"/>
    <exclude body1="goal_mocap" body2="obstacle_1"/>
    <exclude body1="goal_mocap" body2="obstacle_2"/>
    <exclude body1="goal_mocap" body2="obstacle_3"/>
    <exclude body1="goal_mocap" body2="obstacle_4"/>
    <exclude body1="goal_mocap" body2="obstacle_5"/>
    <exclude body1="goal_mocap" body2="obstacle_6"/>
    <exclude body1="goal_mocap" body2="obstacle_7"/>
    <exclude body1="goal_mocap" body2="obstacle_8"/>
    <exclude body1="goal_mocap" body2="obstacle_9"/>
  </contact>

  <equality>
    <weld body1="goal_mocap" body2="goal"/>
    <weld body1="obstacle_0_mocap" body2="obstacle_0"/>
    <weld body1="obstacle_1_mocap" body2="obstacle_1"/>
    <weld body1="obstacle_2_mocap" body2="obstacle_2"/>
    <weld body1="obstacle_3_mocap" body2="obstacle_3"/>
    <weld body1="obstacle_4_mocap" body2="obstacle_4"/>
    <weld body1="obstacle_5_mocap" body2="obstacle_5"/>
    <weld body1="obstacle_6_mocap" body2="obstacle_6"/>
    <weld body1="obstacle_7_mocap" body2="obstacle_7"/>
    <weld body1="obstacle_8_mocap" body2="obstacle_8"/>
    <weld body1="obstacle_9_mocap" body2="obstacle_9"/>
  </equality>

  <actuator>
    <motor name="x_motor" joint="root_x" gear="1" ctrllimited="true" ctrlrange="-1 1"/>
    <motor name="y_motor" joint="root_y" gear="1" ctrllimited="true" ctrlrange="-1 1"/>
  </actuator>
</mujoco>
